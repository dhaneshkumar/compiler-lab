23: atomic_expression ->  variable
24: atomic_expression ->  constant
25: executable_statement_list ->  assignment_statement_list
26: executable_statement_list (RETURN) ->  assignment_statement_list RETURN ';'
27: executable_statement_list (GOTO) ->  assignment_statement_list GOTO BB ';'
28: executable_statement_list (';') ->  assignment_statement_list if_else_clause ';'
29: assignment_statement_list ->  <empty>
30: assignment_statement_list ->  assignment_statement_list assignment_statement
31: assignment_statement (ASSIGN_OP) ->  variable ASSIGN_OP variable ';'
