30: assignment_statement (ASSIGN_OP) ->  variable ASSIGN_OP relop_expression ';'
31: variable (NAME) ->  NAME
32: constant (INTEGER_NUMBER) ->  INTEGER_NUMBER
33: program_$ ->  program


